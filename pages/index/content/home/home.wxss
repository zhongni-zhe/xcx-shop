
.home-menu {
  border-top:10rpx solid #E6E6E6;
  border-bottom:10rpx solid #E6E6E6;
  display:flex;
  width: 100%;
  height:80rpx;
}
.home-menu view{
  flex: 1;
  text-align:center;
  line-height:80rpx;
  font-size:22rpx;
}

.home-demos {
  overflow:hidden;
  margin-top:0rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  padding: 0 10rpx;
}
.home-demos view{
  width:50%;
  height:300rpx;
  float:left;
  background: #fff;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
}
.home-demos view .demo-image {
  width: 100%;
}